package com.nexteducation.NextConfig.module.model.request.Caste;

import org.hibernate.validator.constraints.NotEmpty;

public class CasteUpdateRequest {
	
	
	 
	@NotEmpty(message="Caste Name should not be empty!")
	private Long id;

	 
	private Long branchId;
	
	
	 
	private String parentCasteId;
	
	@NotEmpty(message="Caste Name should not be empty!")
	private String name;



	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}


	public  Long getBranchId() {
		return branchId;
	}

	public void setBranchId(Long branchId) {
		this.branchId = branchId;
	}

	public String getParentCasteId() {
		return parentCasteId;
	}

	public void setParentCasteId(String parentCasteId) {
		this.parentCasteId = parentCasteId;
	}

	public Long getId() {
		return id;
	}

	public void setId(Long id) {
		this.id = id;
	}

}
